{{#> layout/default title="Devflix" background-black=true description="Devflix tag and category page"}}
	{{#*inline "content"}}
		<main class="home no-padding" role="main">
			<div class="col-md-12 empty-div">
			</div>
			<h1 class="title-category">{{#eq type 'tag'}}#{{else}}/{{/eq}}  {{parameter}}</h1>
			<section id="grid" class="row-main flex-grid" data-type="{{type}}" data-parameter="{{parameter}}"></section>
			<div id="pagination" class="pagination pagination-home"></div>
		</main>
	{{/inline}}
	{{#*inline "js"}}
		<script src="/assets/js/dev-post-list.js"></script>
	{{/inline}}
{{/layout/default}}