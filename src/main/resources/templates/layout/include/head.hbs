<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta name="description" content="{{{description}}}">
<meta name="keywords" content="development,web">
<!-- Social: Twitter -->
<meta name="twitter:card" content="summary_large_image">
<meta name="twitter:title" content="{{{title}}}">
<meta name="twitter:description" content="{{{description}}}">
<meta property="twitter:image:src" content="/assets/img/devflix-image.png">
<meta name="twitter:site" content="devflix">
<!-- Social: Facebook / Open Graph -->
<meta property="og:url" content="{{url}}">
<meta property="og:title" content="{{{title}}}">
<meta property="og:image" content="/assets/img/devflix-image.png">
<meta property="og:description" content="{{{description}}}">
<meta property="og:site_name" content="devflix">
<!-- Favicon -->
<link rel="shortcut icon" href="/assets/img/icons/favicon.ico" type="image/x-icon" />

